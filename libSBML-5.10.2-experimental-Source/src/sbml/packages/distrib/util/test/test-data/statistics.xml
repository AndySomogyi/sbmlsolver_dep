      distrib:required="true">
  <model id="UserDefined" name="User Defined Example">
    
    <listOfParameters>
      <parameter id="V" constant="true">
        <distrib:uncertainty>
          <UncertML xmlns="http://www.uncertml.org/3.0">
            <StatisticsCollection definition="http://www.uncertml.org/statistics">
              <Mean definition="http://www.uncertml.org/statistics/mean">
                <value><var varId="V_pop"/></value>
              </Mean>
              <Variance definition="http://www.uncertml.org/statistics/variance">
                <value><var varId="V_omega"/></value>
              </Variance>
            </StatisticsCollection>
          </UncertML>
        </distrib:uncertainty>
      </parameter>
      
      
    </listOfParameters>
    
  </model>
